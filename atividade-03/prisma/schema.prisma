// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id          Int    @id @default(autoincrement())
  client      String
  description String
  protocol    String @unique
  status      Status
}

enum Status {
  IN_PREPARATION
  READY
  CANCELLED
}
